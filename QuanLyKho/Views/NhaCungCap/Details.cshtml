@model QuanLyKho.ViewModels.NhaCungCapViewModel.NhaCungCapViewModel

<div class="container mt-4">
    <div class="col-md-12">
        <h2>Chi tiết nhà cung cấp</h2>
        <p><strong>Mã nhà cung cấp:</strong> @Model.MaNhaCungCap</p>
        <p><strong>Tên nhà cung cấp:</strong> @Model.TenNhaCungCap</p>
        <p><strong>Địa chỉ:</strong> @Model.DiaChi</p>
        <p><strong>Số điện thoại:</strong> @Model.SoDienThoai</p>
        <p><strong>Email:</strong> @Model.Email</p>
    </div>
    <div class="d-flex justify-content-start mt-4">
        <a asp-action="Index" class="btn btn-secondary">Trở về</a>
    </div>
</div>